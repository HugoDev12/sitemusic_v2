{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block stylesheets %}{{ parent() }}
<link rel="stylesheet" type="text/css" href="{{ asset('css/home.css') }}">
{% endblock stylesheets %}


{% block body %}

<div class="example-wrapper ">

    <section class="container-fluid pb-5" id="container-home">

        {% for label, messages in app.flashes(['success', 'danger', 'info', 'warning']) %}
            {% for message in messages %}
                <div class="flash-{{ label }} alert alert-{{ label }}" role="alert">
                    <p class="text-center" style="font-size: 2rem;">{{ message }}</p>
                </div>
            {% endfor %}
        {% endfor %}

            <div class="d-flex justify-content-center  justify-content-xl-start pt-3 mb-5" id="homeHeader">
               <h1 class="ms-xl-5">www.WePlayMusic.fr</h1>
            </div>

        <div class="row" id ="textHomeRow">
            <div class="col-12 col-md-6 d-flex align-items-center">
                <figure>
                    <img id="img-home" src="{{asset('images/band.png')}}" alt="groupe de musique"></img>
                </figure>
            </div>
            <div class="col-12 col-md-6">
                <p class="pt-4 text-justify" id="textHome">Bienvenue sur <span class="fs-3">WePlayMusic.fr</span>, le <span id="spanNouveau">nouveau</span> site d'annonces créé par deux francais <img src="{{asset('images/france.png')}}" alt="drapeau de la France" id="flagFranceHome"></img>, pour trouver son groupe de musique!</br>
                Alors faites chauffer vos guitares, accordez votre piano, faites vos vocalises... vous êtes au bon endroit pour dénicher vos futurs partenaires de musique.</br>
                N'hésitez plus! Postez votre annonce ou postulez à celles en cours! Et partagez de supers moments entre musiciens!</br>
                Let's the music play !</p>
            </div>
        </div>
        
        <div class="row d-flex justify-content-center my-5">
            <div class="col-11 d-flex justify-content-center">
                <a href="{{path('app_advertisement')}}" id="btnAnnonces">Voir les annonces</a>
            </div>
        </div>


